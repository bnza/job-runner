{
  "name": "bnza/job-runner",
  "license": "GPL-3",
  "type": "symfony-bundle",
  "description": "Doctrine simple job runner",
  "keywords": ["job", "task", "runner"],
  "require-dev": {
    "phpunit/phpunit":               "^7.4",
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "Bnza\\JobRunnerBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bnza\\JobRunnerBundle\\Tests\\": "/Tests/"
    }
  },
  "require": {
    "symfony/filesystem": "^4.1",
    "symfony/event-dispatcher": "^4.1",
    "symfony/dotenv": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/config": "^4.1",
    "symfony/yaml": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "doctrine/inflector": "^1.3"
  }
}
